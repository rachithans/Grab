{
    "name": "plugin project",
    "version": "1.0",
    "description": "Build a video downloader",
    "manifest_version": 2,

    "permissions": [
        "<all_urls>",
        "downloads"
      ],
      "background":  {
        "scripts": [
          "./background.js"
      ]
  },
    "browser_action": {
      "default_icon": "icon.png",
        "default_popup": "popup.html"
      }
  }